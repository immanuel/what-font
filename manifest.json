{
    "manifest_version": 2,

    "name": "What Font",
    "description": "Identify fonts from websites that appeal to you",
    "version": "1.0",

    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["lib/jquery.min.js", "getFonts.js"]
        }
    ]
}
